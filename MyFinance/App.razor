@using Supabase.RestAPI
@using System.Text.Json
<Router AppAssembly="@typeof(Program).Assembly"
        AdditionalAssemblies="@(new[] { typeof(MyFinace.Modules.Goals.GoalEntity).Assembly })"> @* Added AdditionalAssemblies *@
